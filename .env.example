# OpenAI API Configuration
OPENAI_API_KEY=sk-your-api-key-here

# OpenAI API Base URL (可选，用于 vLLM 等兼容服务)
# 示例: OPENAI_API_BASE=http://localhost:8000/v1
OPENAI_API_BASE=

# DashScope API Configuration (用于 Qwen 模型)
DASHSCOPE_API_KEY=

# Agent Backend Configuration
# 可选值: openai (OpenAI Agents SDK) | qwen (Qwen-Agent)
AGENT_BACKEND=openai

# Agent Model Configuration
# OpenAI 后端: gpt-4o, gpt-4o-mini, gpt-3.5-turbo
# Qwen 后端: qwen-plus, qwen-turbo, qwen-max
AGENT_MODEL=gpt-4o

# Logging
LOG_LEVEL=INFO

# Observability
ENABLE_TRACING=false

# Session Backend (memory | sqlite)
SESSION_BACKEND=memory

# API Configuration (optional)
API_HOST=0.0.0.0
API_PORT=8000

